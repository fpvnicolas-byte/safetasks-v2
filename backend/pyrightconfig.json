{
  "include": ["."],
  "exclude": ["__pycache__", ".pytest_cache", "node_modules"],
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "none",
  "pythonPlatform": "Linux",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.11",
      "extraPaths": ["."]
    }
  ],
  "venvPath": ".",
  "venv": ".venv"
}
